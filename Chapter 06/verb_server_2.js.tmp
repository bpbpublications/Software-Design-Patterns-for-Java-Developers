const h = require('http')

function onGet(q, r)  {
  r.end('GET invoked')
}
function onPost(q, r)  {
  r.end('POST invoked')
}

const s = h.createServer((q, r) => {
  if (q.method == 'GET')
    onGet(q, r)
  else if (q.method == 'POST')
    onPost(q, r)
  else
    r.end('default response!')
})
s.listen(12000)
