const h = require('http')
function onFoo(q, r)  {
  r.end('foo invoked')
}
function onBar(q, r)  {
  r.end('bar invoked')
}
const s = h.createServer((q, r) => {
  if (q.url == '/foo')
    onFoo(q, r)
  else if (q.url == '/bar')
    onBar(q, r)
  else
    r.end('default response!')
})
s.listen(12000)
