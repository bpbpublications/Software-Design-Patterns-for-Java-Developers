const h = require('http')
const wss = require('websocket')
const s = h.createServer((q, r) => {
  r.end(require('fs').readFileSync(process.argv[2]))
})
s.listen(12000)

console.log(wss)
const ws = new wss.server({ httpServer: s })
ws.on('connection', (ws) => {
  ws.on('message', function incoming(message) {
    console.log('received: %s', message);
  });

  ws.send('something');
});
