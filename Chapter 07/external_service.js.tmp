const h = require('http')
const hs = require('https')
const u = 'https://api.twitter.com/2/tweets/search/stream'
const s = h.createServer((q, r) => {
  hs.get(u, {apikey:'a09bfde1'}, (m) => {
    let data = '';
    m.on('data', (d) => {
      data += d
    })
    m.on('end', () => {
      r.end(data.toString())
    })
  })
})
s.listen(12000)
