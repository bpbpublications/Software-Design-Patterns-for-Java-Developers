const h = require('http')
const d = require('mydbclient')
const u = 'http://mydb.remote:9000/u=user&p=password'
const s = h.createServer((q, r) => {
  const c = d.connect(u)
  c.query({name: 'foo'}, (m) => {
    let data = '';
    m.on('data', (d) => {
      data += d
    })
    m.on('end', () => {
      r.end(data.toString())
    })
  })
})
s.listen(12000)
